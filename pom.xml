<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.jbpm.start</groupId>
  <artifactId>jbpm-bootstrap-kjar</artifactId>
  <version>1.0.0</version>
  <packaging>kjar</packaging>
  <name>bootstrap-jbpm</name>
  <description>Processes responsible for generating business applications</description>
  <dependencies>
    <dependency>
      <groupId>org.kie</groupId>
      <artifactId>kie-api</artifactId>
      <version>7.9.0.Final</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>org.jbpm.contrib</groupId>
      <artifactId>mavenembedder-workitem</artifactId>
      <version>7.10.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>org.jbpm.start</groupId>
      <artifactId>jbpm-bootstrap-model</artifactId>
      <version>1.0.0</version>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.kie</groupId>
        <artifactId>kie-maven-plugin</artifactId>
        <version>7.9.0.Final</version>
        <extensions>true</extensions>
      </plugin>
    </plugins>
  </build>
</project>
